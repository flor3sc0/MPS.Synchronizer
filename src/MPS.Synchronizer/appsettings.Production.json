{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System.Net.Http": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    }
  },

  "ConnectionStrings": {
    "PostgresConnection": {
      "connectionString": "Host=${POSTGRES_HOST};Port=5432;Uid=${POSTGRES_USER};Password=${POSTGRES_PASSWORD};Database=mps-db;"
    }
  },

  "Wb": {
    "Api": {
      "Statistics": "https://statistics-api.wildberries.ru/"
    },
    "LegalEntities": [
      {
        "Token": "${LEGAL_ENTITY_TOKEN_1}",
        "Name": "${LEGAL_ENTITY_1}",
        "PingJobCron": "* * * * *",
        "StatisticsIncomesSyncJobCron": "0 00 * * *",
        "StatisticsStocksSyncJobCron": "0 00 * * *",
        "StatisticsOrdersSyncJobCron": "0 00 * * *",
        "StatisticsSalesSyncJobCron": "0 00 * * *",
        "StatisticsRealizationReportSyncJobCron": "0 00 * * *"
      },
      {
        "Token": "${LEGAL_ENTITY_TOKEN_2}",
        "Name": "${LEGAL_ENTITY_2}",
        "PingJobCron": "* * * * *",
        "StatisticsIncomesSyncJobCron": "0 00 * * *",
        "StatisticsStocksSyncJobCron": "0 00 * * *",
        "StatisticsOrdersSyncJobCron": "0 00 * * *",
        "StatisticsSalesSyncJobCron": "0 00 * * *",
        "StatisticsRealizationReportSyncJobCron": "0 00 * * *"
      }
    ]
  }
}
